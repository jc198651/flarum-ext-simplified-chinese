{
    "name": "jc198651/flarum-ext-simplified-chinese",
    "type": "flarum-extension",
    "description": "Simplified Chinese language pack for Flarum",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "jc198651",
            "email": "jc198651@qq.com"
        }
    ],
    "support": {
        "source": "https://github.com/jc198651/flarum-ext-simplified-chinese"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		},
        "flarum-extension": {
            "title": "Chinese (zh_CN)",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "zh-cn",
            "title": "简体中文"
        }
    }
}
